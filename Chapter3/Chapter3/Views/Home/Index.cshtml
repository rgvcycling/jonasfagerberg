@model IEnumerable<Chapter3.Models.Video>

<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>Video</title>
</head>
<body>
    <table>
			Available Videos
			@foreach (var video in Model) 
			{
		        <tr>
                    <td>@video.Id.</td>
                    <td>@video.Title</td>
                </tr>	
			}
			        
		
	</table>
</body>
</html>