@model IEnumerable<Chapter3.ViewModels.VideoViewModel>

<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>Video</title>
</head>
<body>
    <table>
			Available Videos</br></br>
			<tr>
				<b>
				    <td><u>ID</u></td>
				    <td><u>Genre</u></td>
				    <td><u>Movie</u></td>
				</b>
			</tr>

			@foreach (var video in Model) 
			{
		        <tr>
                    <td>@video.Id.</td>
				    <td>@video.Genre</td>
                    <td>@video.Title</td>
                </tr>	
			}
			        
		
	</table>
</body>
</html>